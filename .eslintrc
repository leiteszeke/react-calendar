{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "amd": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "expect": true,
        "it": true,
        "describe": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
        "react/no-string-refs": 0,
        "comma-dangle": 0,
        "react/jsx-uses-vars": 1,
        "react/display-name": 0,
        "no-unused-vars": "warn",
        "no-console": 1,
        "no-unexpected-multiline": "warn",
        "react/prop-types": 0,
        "react/no-find-dom-node": 0
    },
    "settings": {
        "react": {
            "pragma": "React",
            "version": "16.6.0"
        }
    }
}